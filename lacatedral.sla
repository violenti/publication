        <PAGEOBJECT XPOS="156.7256" YPOS="16322" OwnPage="36" ItemID="964390438" PTYPE="4" WIDTH="492.75" HEIGHT="367.5" FRTYPE="0" CLIPEDIT="0" PWIDTH="1" PLINEART="1" LOCALSCX="1" LOCALSCY="1" LOCALX="0" LOCALY="0" LOCALROT="0" PICART="1" SCALETYPE="1" RATIO="1" COLUMNS="1" COLGAP="0" AUTOTEXT="0" EXTRA="0" TEXTRA="0" BEXTRA="0" REXTRA="0" VAlign="0" FLOP="0" PLTSHOW="0" BASEOF="0" textPathType="0" textPathFlipped="0" path="M0 0 L492.75 0 L492.75 367.5 L0 367.5 L0 0 Z" copath="M0 0 L492.75 0 L492.75 367.5 L0 367.5 L0 0 Z" gXpos="156.7256" gYpos="16322" gWidth="0" gHeight="0" LAYER="0" NEXTITEM="-1" BACKITEM="-1">
            <StoryText>
                <DefaultStyle/>
                <ITEXT FONT="FreeMono Regular" CH="$Id: cathedral-bazaar.sgml,v 1.39 1998/07/28 05:04:58 esr Exp $"/>
                <para ALIGN="3"/>
                <ITEXT FONT="FreeMono Regular" CH="Expuse 1.17 en el Linux Kongress, realizado el 21 de Mayo de 1997."/>
                <para ALIGN="3"/>
                <ITEXT FONT="FreeMono Regular" CH="Agregue la bibliografía el 7 de Julio de 1997."/>
                <para ALIGN="3"/>
                <para ALIGN="3"/>
                <ITEXT FONT="FreeMono Regular" CH="Puse la anécdota de la Conferencia de Perl el 18 de Noviembre de 1997."/>
                <para ALIGN="3"/>
                <ITEXT FONT="FreeMono Regular" CH="Sustituí el término de &quot;software gratuito&quot; por el de &quot;fuente abierta&quot; el día 9 de Febrero de 1998 en la versión 1.29."/>
                <para ALIGN="3"/>
                <para ALIGN="3"/>
                <ITEXT FONT="FreeMono Regular" CH="Agregué la sección &quot;Epílogo: Netscape Adopta el Bazar!&quot;"/>
                <var name="pgno" FONT="FreeMono Regular"/>
                <ITEXT FONT="FreeMono Regular" CH=" el día 10 de Febrero de 1998 en la versión 1.31."/>
                <para ALIGN="3"/>
                <para ALIGN="3"/>
                <ITEXT FONT="FreeMono Regular" CH="Eliminé la gráfica de Paul Eggert sobre GPL vs. Bazar como respuesta a argumentos reiterados por parte de RMS el día 28 de Julio de 1998."/>
                <para ALIGN="3"/>
                <ITEXT FONT="FreeMono Regular" CH="En otras revisiones he incorporado cambios editoriales menores y corregido algunos detalles."/>
                <para ALIGN="3"/>
                <para ALIGN="3"/>
                <ITEXT FONT="FreeMono Regular" CH="Se actualizaron links de referencia segun versión en ingles del blog de Raymond, donde decia http://alpha-bits.ai.mit.edu/articles/good-news/good-news.html. ahora es http://naggum.no/worse-is-better.html"/>
                <trail ALIGN="3"/>
            </StoryText>
        </PAGEOBJECT>
