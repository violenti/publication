        <PAGEOBJECT XPOS="734.560606060606" YPOS="137.0016" OwnPage="1" ItemID="920437734" PTYPE="4" WIDTH="512.878787878788" HEIGHT="241.1064" FRTYPE="0" CLIPEDIT="0" PWIDTH="1" PLINEART="1" LOCALSCX="1" LOCALSCY="1" LOCALX="0" LOCALY="0" LOCALROT="0" PICART="1" SCALETYPE="1" RATIO="1" COLUMNS="1" COLGAP="0" AUTOTEXT="0" EXTRA="0" TEXTRA="0" BEXTRA="0" REXTRA="0" VAlign="0" FLOP="0" PLTSHOW="0" BASEOF="0" textPathType="0" textPathFlipped="0" path="M0 0 L512.879 0 L512.879 241.106 L0 241.106 L0 0 Z" copath="M0 0 L512.879 0 L512.879 241.106 L0 241.106 L0 0 Z" gXpos="734.560606060606" gYpos="137.0016" gWidth="0" gHeight="0" LAYER="0" NEXTITEM="-1" BACKITEM="-1">
